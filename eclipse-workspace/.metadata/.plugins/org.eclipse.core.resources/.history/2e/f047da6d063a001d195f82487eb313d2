
public class Resolucion {
	public static void main(String[] args) {
		Almacenamiento almacenamiento=GestorFile.leer("./PreparacionPruebas/LotePrueba/Entrada/01_Enunciado.in");
		
		System.out.println(almacenamiento.getCantidadAplicaciones());
		for(Aplicacion a: almacenamiento.getCelu().getAplicaciones())
			System.out.println(a.getMemoria());
	}
}
