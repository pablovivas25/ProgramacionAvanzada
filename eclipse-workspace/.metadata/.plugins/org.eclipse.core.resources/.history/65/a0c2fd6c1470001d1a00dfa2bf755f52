package paquete;

public class Cougar extends Feline {
	public Cougar() {
		System.out.print("cougar ");
	}

	public static void main(String[] args) {
		new Cougar().go();
	}

	void go() {
		type = "d ";
		///type2="B";
		///Se llama al constructor del padre Feline y como el atributo es publico se modifica en ambos lados
		System.out.print(this.type + super.type);
	}
}